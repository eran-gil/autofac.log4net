			</group>
			<group targetFramework=".NETFramework4.5">
				<dependency id="Autofac" version="3.0.0" />
				<dependency id="log4net" version="2.0.3" />
			</group>
			<group targetFramework=".NETStandard2.0">
				<dependency id="Autofac" version="4.0.0" />
				<dependency id="log4net" version="2.0.6" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="bin\Autofac.log4net.dll" target="lib\net40"/>
		<file src="bin\Autofac.log4net.pdb" target="lib\net40"/>
